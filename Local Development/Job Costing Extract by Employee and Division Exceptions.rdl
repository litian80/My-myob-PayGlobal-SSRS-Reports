    <DataSet Name="paramEmployee">
      <Query>
        <DataSourceName>PayGlobal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@IncludeTerm">
            <Value>=Parameters!IncludeTerm.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DivisionCode">
            <Value>=Parameters!DivisionCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @IncludeTerm = 1
BEGIN
SELECT E.EmployeeCode, E.LastName + ', ' + PG.FirstName(E.FirstNames) + ' (' + E.EmployeeCode + ')' AS EmployeeName
FROM Employee E
WHERE E.Dim1Code IN (@DivisionCode)
ORDER BY E.LastName, E.FirstNames
END

IF @IncludeTerm = 0
BEGIN
SELECT E.EmployeeCode, E.LastName + ', ' + PG.FirstName(E.FirstNames) + ' (' + E.EmployeeCode + ')' AS EmployeeName
FROM Employee E
WHERE E.Dim1Code IN (@DivisionCode) AND E.TerminationDate IS NULL
ORDER BY E.LastName, E.FirstNames
END</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeCode">
          <DataField>EmployeeCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmployeeName">
          <DataField>EmployeeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
