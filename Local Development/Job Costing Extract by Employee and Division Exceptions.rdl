    <DataSet Name="dataJobCosting">
      <Query>
        <DataSourceName>PayGlobal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PaySeq">
            <Value>=Parameters!PaySeq.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@JobCostAllow">
            <Value>=Parameters!JobCostAllow.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
/* debug comment out in production
use PG1251NZ

declare @JobCostAllow varchar(12) = 'JOBALLW'
	,	 @JobCostHours varchar(12) = 'JOBHOUR'
	,	@PaySeq varchar(12) = '11'
--*/




select ha.EmployeeCode
,	ha.Date
,	ha.allowanceCode
,	ha.PaySequence
,	ha.Quantity
,	ha.TotalAmount
,	ha.HistoricalAllowanceID

from HistoricalAllowance ha
inner join AllowanceGroupAllowance ag on ag.AllowanceCode=ha.AllowanceCode and ag.AllowanceGroupCode = @JobCostAllow
where 
	ha.Quantity = 0
and	ha.PaySequence in (@PaySeq)

order by ha.EmployeeCode
</CommandText>
      </Query>
      <Fields>
        <Field Name="EmployeeCode">
          <DataField>EmployeeCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PaySequence">
          <DataField>PaySequence</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalAmount">
          <DataField>TotalAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="HistoricalAllowanceID">
          <DataField>HistoricalAllowanceID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AllowanceCode">
          <DataField>allowanceCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
