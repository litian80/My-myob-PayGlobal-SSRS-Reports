    <DataSet Name="dataJobCosting">
      <Query>
        <DataSourceName>PayGlobal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@JobCostHours">
            <Value>=Parameters!JobCostHours.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PaySeq">
            <Value>=Parameters!PaySeq.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@JobCostAllow">
            <Value>=Parameters!JobCostAllow.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
/* debug comment out in production
use PG1251NZ_test2

declare @JobCostAllow varchar(12) = 'JOBALLW'
	,	 @JobCostHours varchar(12) = 'JOBHOUR'
	,	@PaySeq varchar(12) = '4'
--*/

if object_id('tempdb..#Proportion') is not null drop table #Proportion
create table #Proportion(
	EmployeeCode varchar(12)
,	PaySequence varchar(12)
,	CostCentreCode varchar(12)
,	TotalQuantityProportion numeric(9,8))

insert into #Proportion
select ha.EmployeeCode
,	ha.PaySequence
,	ha.CostCentreCode
,	ha.Quantity
/	sum(ha.Quantity) over (partition by ha.EmployeeCode, ha.PaySequence) as TotalQuantityProportion
from HistoricalAllowance ha
inner join AllowanceGroupAllowance ag on ag.AllowanceCode=ha.AllowanceCode and ag.AllowanceGroupCode = @JobCostHours
where ha.Quantity &lt;&gt; 0 and ha.PaySequence in (@PaySeq)


if object_id('tempdb..#HistoricalAllowanceDetails') is not null drop table #HistoricalAllowanceDetails

create table #HistoricalAllowanceDetails (
		EmployeeCode varchar(12)
	,	PaySequence varchar(12)
	,	Quantity numeric(11,4)
	,	Amount numeric(11,4)
	,	AllowanceCode varchar(12)
	,	CostCentreCode varchar(12)
	,	HistoricalAllowanceID int
)

insert into #HistoricalAllowanceDetails
select ha.EmployeeCode
,	ha.PaySequence
,	isnull(p.TotalQuantityProportion * ha.Quantity, ha.Quantity) as Quantity
,	isnull(p.TotalQuantityProportion * ha.TotalAmount, ha.TotalAmount) as Amount
,	ha.AllowanceCode
,	p.CostCentreCode
,	ha.HistoricalAllowanceID

from HistoricalAllowance ha
inner join AllowanceGroupAllowance ag on ag.AllowanceCode=ha.AllowanceCode and ag.AllowanceGroupCode = @JobCostAllow
left join #Proportion p on p.EmployeeCode COLLATE DATABASE_DEFAULT=ha.EmployeeCode COLLATE DATABASE_DEFAULT
where 
	ha.TotalAmount &lt;&gt; 0
and	ha.PaySequence in (@PaySeq)
order by ha.EmployeeCode

/*Begining of Report Data Output*/

select hd.CostCentreCode
,	tpps.PeriodEnd
,	hd.AllowanceCode
,	al.Description as Allowance
,	cc.Job as JobNumber
,	cc.Activity as [CostCodeActivity]
,	cc.Zone as [CostCodeZone]
,	sum(hd.Amount) as Amount
,	sum(hd.Quantity) as Quantity
from #HistoricalAllowanceDetails hd
inner join Allowance al on al.AllowanceCode COLLATE DATABASE_DEFAULT = hd.AllowanceCode COLLATE DATABASE_DEFAULT
inner join TransPerPaySequence tpps on tpps.PaySequence=hd.PaySequence
left join pg.CostCentreDescriptionCodesLU cc on cc.CostCentreCode COLLATE DATABASE_DEFAULT=hd.CostCentreCode COLLATE DATABASE_DEFAULT

group by hd.AllowanceCode
,	al.Description
,	hd.CostCentreCode
,	tpps.PeriodEnd
,	hd.AllowanceCode
,	cc.Job
,	cc.Activity
,	cc.Zone</CommandText>
      </Query>
      <Fields>
        <Field Name="CostCentreCode">
          <DataField>CostCentreCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PeriodEnd">
          <DataField>PeriodEnd</DataField>
