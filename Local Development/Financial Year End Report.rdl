    <DataSet Name="ParameterAllowance">
      <Query>
        <DataSourceName>PG1251NZ_Test2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FinancialYTDCode">
            <Value>=Parameters!FinancialYTDCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

select distinct ha.AllowanceCode
,	al.[Description] as Allowance
from HistoricalAllowance ha
inner join Allowance al on al.AllowanceCode = ha.AllowanceCode
inner join TransPerPaySequence tpps on tpps.PaySequence=ha.PaySequence

where 
 al.Taxable = 1 and ha.TotalAmount &lt;&gt; 0
and tpps.FinancialYTDCode = @FinancialYTDCode</CommandText>
      </Query>
