    <DataSet Name="ParameterFinancialYTD">
      <Query>
        <DataSourceName>PG1251NZ_Test2</DataSourceName>
        <CommandText>select PayPeriodFinancialYTDCode
,	[Description] as PayPeriodFinancialYTD

from PayPeriodFinancialYTD
where DateFrom &lt; GETDATE()
order by DateFrom desc</CommandText>
      </Query>
