    <DataSet Name="ParameterPayPeriod">
      <Query>
        <DataSourceName>PG1251NZ_Test2</DataSourceName>
        <CommandText>


select PayPeriodCode
,	[Description] as PayPeriod
from PayPeriod
where PayPeriodCode in ('TPK','CONTRACTORS')</CommandText>
      </Query>
