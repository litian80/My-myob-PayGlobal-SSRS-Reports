  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="PG1251NZ_Test2">
      <DataSourceReference>PG1251NZ_Test2</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>3f989d23-3e9e-46f9-8389-e5af2ad1e4e4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>PG1251NZ_Test2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FinancialYTDCode">
            <Value>=Parameters!FinancialYTDCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllowanceCode">
            <Value>=Parameters!AllowanceCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PayPeriodCode">
            <Value>=Parameters!PayPeriodCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InclTerm">
            <Value>=Parameters!InclTerm.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
declare @InclTerm int
set @InclTerm = 1
--*/

	select e.EmployeeCode
	,	e.LastName +', '+ e.FirstNames as Name
	,	fy.[Description] as FinancialYear
	,	e.CostCentreCode
	,	es.[Description] as EmployeeStatus
	,	e.StartDate
	,	e.EmployeeStatusCode
	,	e.TotalProfiledHours/40 as FTE
	,	e.TerminationDate
	,	tr.[Description] as TerminationReason
	,	ha.AllowanceCode
	,	al.[Description] as Allowance
	,	ha.TotalAmount
	,	case when e.TerminationDate is null then 0 else 1 end as Terminated
	from Employee e
	inner join EmployeeStatus es on es.EmployeeStatusCode=e.EmployeeStatusCode
	inner join HistoricalAllowance ha on ha.EmployeeCode=e.EmployeeCode
	inner join Allowance al on al.AllowanceCode=ha.AllowanceCode
	inner join TransPerPaySequence tpps on tpps.PaySequence = ha.PaySequence
	left join TermReason tr on tr.TermReasonCode=e.TermReasonCode

	inner join PayPeriodFinancialYTD fy on fy.PayPeriodFinancialYTDCode=tpps.FinancialYTDCode

	where al.Taxable = 1 and ha.TotalAmount &lt;&gt; 0
	and tpps.FinancialYTDCode = @FinancialYTDCode
	and ha.AllowanceCode in (@AllowanceCode)
	and e.PayPeriodCode = @PayPeriodCode
	--/*
	and 
	case when e.TerminationDate is null then 0 else 1 end = 
	case @InclTerm
		when 0 then 0 
		When 1 then case when e.TerminationDate is null then 0 else 1 end
	end
	--*/
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
