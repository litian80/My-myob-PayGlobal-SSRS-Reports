  <DataSets>
    <DataSet Name="dsEmplUnscheduledLeave">
      <Query>
        <DataSourceName>PayGlobal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmployeeCode">
            <Value>=Parameters!EmployeeCode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AllowanceCode">
            <Value>=Parameters!AllowanceCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/* Select the set of all employee allowances where the startdate and enddate (date) fields are the same */
DECLARE @rEmpAllowances TABLE ( EmployeeCode    Varchar(12) --PRIMARY KEY WITH (IGNORE_DUP_KEY = ON) 
                               ,StartDate       Datetime
                               ,LeaveEndDate    Datetime 
                               ,AllowanceCode   Varchar(12))
INSERT INTO @rEmpAllowances
SELECT   HA.EmployeeCode
        ,HA.StartDate
        ,[Date] AS LeaveEndDate
        ,HA.AllowanceCode
FROM HistoricalAllowance    as ha
JOIN Allowance              as a    ON HA.AllowanceCode = a.AllowanceCode
WHERE HA.StartDate = HA.[Date]
  AND LEFT(A.Type, 1) IN ('L','S','V','O')
       AND (HA.EmployeeCode IN (@EmployeeCode))
       AND (HA.StartDate BETWEEN @StartDate AND @EndDate)
       AND (HA.AllowanceCode IN (@AllowanceCode))

/* */
DECLARE @DOW TABLE (EmployeeCode varChar(12) PRIMARY KEY WITH (IGNORE_DUP_KEY = ON), 
                    EmployeeName varChar(100), 
                    Mon Int, Tues Int, Wed Int, Thurs Int, Fri Int, Sat Int, Sun Int)
INSERT INTO @DOW
SELECT Code,
       Name,
       SUM(Mon) AS 'Mon',
       SUM(Tues) AS 'Tues',
       SUM(Wed) AS 'Wed',
       SUM(Thurs) AS 'Thurs',
       SUM(Fri) AS 'Fri',
       SUM(Sat) AS 'Sat',
       SUM(Sun) AS 'Sun'
FROM (SELECT E.EmployeeCode AS Code, ISNULL(E.PreferredName, E.FirstNames) + ' ' + E.LastName AS Name, 
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'mon%' THEN 1 ELSE 0 END AS Mon,
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'tue%' THEN 1 ELSE 0 END AS Tues,
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'wed%' THEN 1 ELSE 0 END AS Wed,
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'thu%' THEN 1 ELSE 0 END AS Thurs,
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'fri%' THEN 1 ELSE 0 END AS Fri,
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'sat%' THEN 1 ELSE 0 END AS Sat,
       CASE WHEN DATENAME(dw,EAV.StartDate) like 'sun%' THEN 1 ELSE 0 END AS Sun
FROM Employee E
JOIN @rEmpAllowances EAV ON E.EmployeeCode = EAV.EmployeeCode 
) AS D
GROUP BY D.Code, D.Name

 
DECLARE @PublicHolidays TABLE (  EmployeeCode varChar(12) PRIMARY KEY WITH (IGNORE_DUP_KEY = ON)
                                ,DayBefore Int
                                ,DayAfter Int)
INSERT INTO @PublicHolidays
SELECT P.Code,  
       SUM(P.[Day before]) AS DayBefore,
       SUM(P.[Day after]) AS DayAfter
FROM
(SELECT E.EmployeeCode AS Code,
       CASE WHEN EAV.StartDate = DATEADD(d,-1,H.HolidayDate) THEN 1 ELSE 0 END AS [Day before],
       CASE WHEN EAV.StartDate = DATEADD(d,1,H.HolidayDate) THEN 1 ELSE 0 END AS [Day after]
FROM Employee E
INNER JOIN @rEmpAllowances EAV ON E.EmployeeCode = EAV.EmployeeCode 
LEFT OUTER JOIN HolidayGroup HG on E.HolidayGroupCode = HG.HolidayGroupCode
LEFT OUTER JOIN HolidayGroupMku HGM on HG.HolidayGroupCode = HGM.HolidayGroupCode
LEFT OUTER JOIN Holiday H  ON H.HolidayCode = HGM.HolidayCode
) AS P
GROUP BY P.CODE;


SELECT D.EmployeeCode, 
       D.EmployeeName AS Name, 
       D.Mon AS Mon, 
       D.Tues AS Tue, 
       D.Wed AS Wed, 
       D.Thurs AS Thu, 
       D.Fri AS Fri, 
       D.Sat AS Sat, 
       D.Sun AS Sun, 
       (D.Mon + D.Tues + D.Wed + D.Thurs + D.Fri + D.Sat + D.Sun) AS Total, 
       PH.DayBefore AS 'Day before', 
       PH.DayAfter AS 'Day after'
FROM @DOW D
LEFT OUTER JOIN @PublicHolidays PH ON D.EmployeeCode = PH.EmployeeCode</CommandText>
